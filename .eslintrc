{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": ["standard", "standard-react"],
  "rules": {
    "space-before-function-paren": "off",
    "react/prop-types": "off",
    "jsx-quotes": ["error", "prefer-double"]
  }
}
